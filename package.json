{
  "name": "remix-template-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run connect:database:develop\" \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o app/styles/app.css",
    "connect:database:develop": "pscale connect shopiee-app develop --port 3309",
    "connect:database:main": "pscale connect shopiee-app main --port 3309",
    "prisma:create-migration": "npx prisma migrate dev",
    "prisma:reset-db": "npx prisma migrate reset",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@remix-run/vercel": "^1.3.4",
    "@vercel/node": "^1.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
